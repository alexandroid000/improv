@article{adelman_examining_1998,
	Abstract = {An experiment was performed testing the {\^a}display cognitive
consistency hypothesis{\^a} proposed in Adelman et al. (1996). This
hypothesis states that the effectiveness of a display format for
decision-aiding systems depends on the consistency between how the
system displays its reasoning process and how the person is processing
the information. Results of an experiment using a simulated Army air
