	Journal = {Proceedings of the {IEEE}},
	Keywords = {compressed medical images, diagnostic accuracy, digital image, distortion measures, image coding, lossy algorithms, medical image processing, medical image quality, {PACS}, picture archiving and communication systems, predictive pruned tree-structured vector quantization, radiology, {SNR}, subjective rating, teleradiology systems, vector quantisation},
	Number = {6},
	Pages = {919--932},
