	Issn = {0033-{295X}},
	Journal = {Psychological Review},
	Keywords = {Experimentation, Experimenter Bias, Hypothesis Testing, information bias vs positive test strategy \& confirmation vs disconfirmation in hypothesis testing \& prediction, Prediction, Strategies},
	Month = apr,
	Number = {2},
	Pages = {211--228},
	Title = {Confirmation, disconfirmation, and information in hypothesis testing.},
	Url = {http://search.ebscohost.com/login.aspx?direct=true&db=pdh&AN=rev-94-2-211&site=ehost-live},
