	File = {Rude 2012 quantifying impact of gap fill radar.pdf:C:\djr7m\sync\LIT\CASA\Rude 2012 quantifying impact of gap fill radar.pdf:application/pdf},
	Journal = {Meteorological Applications},
	Keywords = {absolute error, affirmative warning decisions, confidence ratings, decision making, ground level wind forecasting, human judgment, meteorological radar, spatio-temporal resolution weather radar data, weather forecasting, weather radar, wind, wind speed, wind speed assessments},
	Month = sep,
	Number = {3},
	Pages = {355--370},
	Title = {Quantifying the impact of adding gap filling radar data on forecaster wind assessments, warnings, and confidence},
	Url = {http://onlinelibrary.wiley.com/doi/10.1002/met.269/abstract},
