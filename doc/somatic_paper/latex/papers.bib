	Keywords = {Animals, camera movements, cameras, Conferences, Detectors, event detection, image sensors, image sequences, Layout, learning (artificial intelligence), object detection, object occlusions, object-specific detector, online learning, Robustness, Stability, Tracking, tracking-modeling-detection, unstable tracking, video sequences, video signal processing, visual tracking},
	Month = oct,
