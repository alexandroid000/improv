	Issn = {0162-8828},
	Journal = {{IEEE} Transactions on Pattern Analysis and Machine Intelligence},
	Keywords = {{ALGORITHMS}, Artificial intelligence, database management systems, Databases, Factual, Documentation, expectation-maximisation algorithm, expectation-maximization algorithm, image annotation, image classification, Image Enhancement, Image Interpretation, Computer-Assisted, image retrieval, Information Storage and Retrieval, learning (artificial intelligence), Natural Language Processing, Pattern Recognition, Automated, semantic classes, Semantics, Sensitivity and Specificity, supervised learning},
