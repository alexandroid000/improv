	File = {Packer 2012 combined pose obect model.pdf:C:\djr7m\sync\LIT\Activity Recognition\Packer 2012 combined pose obect model.pdf:application/pdf},
	Keywords = {action understanding, cooking action dataset, depth readings, feature model, fine-grained human actions, gesture recognition, Kinect sensor, natural human activity, object manipulations, object model, object tracking, pose estimation, pose model, pose trackers, pose tracks, pose trajectories, realistic action sequences, Sensors, video},
