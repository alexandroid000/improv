	File = {ryoo 2009 spatio-temporal relationship match.pdf:C:\djr7m\sync\LIT\Activity Recognition\ryoo 2009 spatio-temporal relationship match.pdf:application/pdf},
	Keywords = {complex human activities recognition, complex nonperiodic activities, human activity recognition, individual pedestrians, multiple interacting persons, spatio-temporal features, spatio-temporal local features, spatio-temporal relationship, structural similarity, video signal processing, video structure comparison},
	Month = oct,
	Pages = {1593 --1600},
	Shorttitle = {Spatio-temporal relationship match},
	Title = {Spatio-temporal relationship match: Video structure comparison for recognition of complex human activities},
	Year = {2009},
	Bdsk-Url-1 = {http://dx.doi.org/10.1109/ICCV.2009.5459361}}

