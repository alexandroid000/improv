	Journal = {Journal of Cognitive Engineering and Decision Making},
	Keywords = {Presentation Format, Probabilistic Information, Reasoning},
	Pages = {209--227},
	Title = {Uncertainty forecasts improve decision making among nonexperts},
	Url = {http://www.ingentaconnect.com/content/hfes/cogeng/2009/00000003/00000003/art00001},
	Volume = {3},
	Year = {2009},
