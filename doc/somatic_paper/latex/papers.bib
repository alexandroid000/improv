	Keywords = {abnormality detection, Activity recognition, Aging, Artificial intelligence, Atomic layer deposition, Buildings, Computational modeling, discrete Coxian distribution, discrete Coxian duration model, explicit duration model, hidden Markov models, high-level activity sequences, human daily living activities, human factors, {HUMANS}, image motion analysis, image recognition, learning, multinomial distribution, Pervasive computing, S-{HSMM}, switching hidden semi-Markov model, Training data, ubiquitous computing},
	Pages = {838--845 vol. 1},
