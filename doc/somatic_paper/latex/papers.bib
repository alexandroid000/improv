	Journal = {Bulletin of the American Meteorological Society},
	Month = jun,
	Number = {6},
	Pages = {580--588},
	Title = {Real-Time Color Doppler Radar Display},
	Url = {http://dx.doi.org/10.1175%2F1520-0477%281975%29056%3C0580%3ARTCDRD%3E2.0.CO%3B2},
	Urldate = {2009-03-16},
