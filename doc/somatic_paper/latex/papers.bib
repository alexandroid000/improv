	Journal = {Journal of Information Science},
	Month = apr,
	Number = {2},
	Pages = {163 --180},
	Shorttitle = {The wisdom hierarchy},
	Title = {The wisdom hierarchy: representations of the {DIKW} hierarchy},
	Url = {http://jis.sagepub.com/content/33/2/163.abstract},
	Urldate = {2010-10-19},
	Volume = {33},
