	Issn = {1094-6977},
	Journal = {{IEEE} Transactions on Systems, Man, and Cybernetics, Part C: Applications and Reviews},
	Keywords = {data molding, image sequences, informative units, pattern recognition, semantic event model, semantic video occurrence, state event model, video abstraction, video event understanding, video sequence, video signal processing},
	Month = sep,
	Number = {5},
	Pages = {489 --504},
