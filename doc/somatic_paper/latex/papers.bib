	Keywords = {Computers / Intelligence ({AI)} \& Semantics, Computers / Interactive \& Multimedia, Computers / Social Aspects / Human-Computer Interaction, Computers / User Interfaces},
	Language = {en},
	Publisher = {Prentice Hall},
	Title = {Human-Computer Interaction},
