	Keywords = {filtering algorithm, filtering theory, human pose markerless tracking, kinematic chain, local model-based search, monocular depth image, motion estimation, pose estimation, programmable graphics hardware, real time motion capture, single time-of-flight camera, unscented transform},
	Month = jun,
	Pages = {755 --762},
	Title = {Real time motion capture using a single time-of-flight camera},
	Year = {2010},
