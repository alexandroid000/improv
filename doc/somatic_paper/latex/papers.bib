	Keywords = {Bayes methods, Bayesian inference, Bayesian super-resolved surface reconstruction, geometrical properties, image data, image reconstruction, inference mechanisms, parameters estimation, simulated aerial photographs, surface reconstruction, three dimensional reconstruction},
	Pages = {375 --382 vol.1},
	Title = {Bayesian super-resolved surface reconstruction from images},
