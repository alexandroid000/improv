	Keywords = {Explicit duration {HMM}, Forward{\^a}backward ({FB)} algorithm, Hidden Markov Model ({HMM)}, Hidden semi-Markov model ({HSMM)}, {HSMM}, Variable duration {HMM}, Viterbi algorithm},
	Month = feb,
	Number = {2},
	Pages = {215--243},
	Shorttitle = {Special Review Issue},
