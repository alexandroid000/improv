	File = {Natarajan 2008 RT tracking human actions.pdf:C:\djr7m\sync\LIT\Yu-cites\Natarajan 2008 RT tracking human actions.pdf:application/pdf},
	Keywords = {computer graphics, Decoding, Delay, gesture recognition, hidden Markov models, hierarchical variable transition, human action recognition, Human motion, {HUMANS}, image motion analysis, Intelligent robots, Markov chain, occlusion, probability, Real time systems, real-time tracking, Robustness, speech recognition, state transition probability, Tracking, variable transition hidden Markov model, Viterbi algorithm, Viterbi decoding},
	Pages = {1--8},
	Title = {Online, Real-time Tracking and Recognition of Human Actions},
