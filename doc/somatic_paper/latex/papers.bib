	Journal = {Systems, Man and Cybernetics, Part A: Systems and Humans, {IEEE} Transactions on},
	Keywords = {{AUTOMATION}, cognitive engineering, function allocation, human computer interaction, human factors, information acquisition, Information analysis, man machine systems, man-machine systems, user interfaces},
	Number = {3},
	Pages = {286--297},
