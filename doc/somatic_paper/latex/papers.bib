	Issn = {1077-2626},
	Journal = {Visualization and Computer Graphics, {IEEE} Transactions on},
	Keywords = {data visualisation, information visualization, interaction cost, multiple input mode costs, physical-motion costs, state-change costs, system-power costs, user interfaces, view-change costs, visual-cluttering costs, visualization design},
