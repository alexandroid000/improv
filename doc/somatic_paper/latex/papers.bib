	Keywords = {Command and Control, critical incident technique, decision making, Decision Strategies, Firefighting, Protocol Analysis, Time Pressure},
	Pages = {186--209},
	Shorttitle = {Rapid Decision Making on the Fire Ground},
	Title = {Rapid Decision Making on the Fire Ground: The Original Study Plus a Postscript},
