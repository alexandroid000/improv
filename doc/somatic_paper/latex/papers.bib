	Isbn = {0743-1619},
	Keywords = {distributed collaborative adaptive sensor network, distributed sensors, Doppler radar, Doppler weather radar, end-user driven approach, meteorological radar, remote sensing application, remote sensing by radar, sit-and-spin volume coverage pattern},
	Pages = {4167--4172},
	Title = {Distributed collaborative adaptive sensor networks for remote sensing applications},
	Year = {2008},
