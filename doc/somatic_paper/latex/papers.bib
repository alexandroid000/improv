	Journal = {Applied Ergonomics},
	Keywords = {Augmented reality, {ERGONOMICS}, real-time},
	Month = jul,
	Number = {4},
	Pages = {566--574},
	Title = {Innovative system for real-time ergonomic feedback in industrial manufacturing},
