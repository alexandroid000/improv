	Issn = {1099-0720(Electronic);0888-4080(Print)},
	Journal = {Applied Cognitive {Psychology;Applied} Cognitive Psychology},
	Keywords = {judgments of proportion using graphs, adults},
	Number = {2},
	Pages = {173--190},
	Shorttitle = {Judging proportion with graphs},
	Title = {Judging proportion with graphs: The summation model},
	Volume = {12},
	Year = {1998},
	Bdsk-Url-1 = {http://dx.doi.org/10.1002/(SICI)1099-0720(199804)12:2%3C173::AID-ACP499%3E3.0.CO;2-K}}

